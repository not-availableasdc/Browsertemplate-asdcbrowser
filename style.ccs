body {
    font-family: sans-serif;
    margin: 0;
    padding: 0;
background: mediumpurple
}

input#addressBar {
    width: 80%;
    padding: 10px;
    font-size: 16px;
    margin-bottom: 10px;
}

button#goButton {
    padding: 10px 20px;
    font-size: 16px;
    background-color: #4CAF50;
    color: white;
    border: none;
    cursor: pointer;
}

iframe#browserFrame {
    width: 100%;
    height: 500px;
    border: 1px solid #ccc;
}
